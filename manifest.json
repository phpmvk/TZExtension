{
  "manifest_version": 2,
  "name": "DOM Element Finder",
  "version": "1.0",
  "description": "Find DOM elements by ID and provide buttons to manipulate them.",
  "permissions": ["activeTab"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
